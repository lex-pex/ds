{
    "name": "lex-pex/ds",
    "description": "PHP Data Structures Library",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "DataStructures\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataStructures\\Tests\\": "tests/"
        }
    },
    "require-dev": {
